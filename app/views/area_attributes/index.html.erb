<h1>Listing area_attributes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @area_attributes.each do |area_attribute| %>
      <tr>
        <td><%= area_attribute.name %></td>
        <td><%= area_attribute.value %></td>
        <td><%= link_to 'Show', area_attribute %></td>
        <td><%= link_to 'Edit', edit_area_attribute_path(area_attribute) %></td>
        <td><%= link_to 'Destroy', area_attribute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

